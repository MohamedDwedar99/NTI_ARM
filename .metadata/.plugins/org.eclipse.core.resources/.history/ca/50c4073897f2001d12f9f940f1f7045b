#ifndef USART_PRIVATE_H_
#define USART_PRIVATE_H_

typedef struct{
	u32 SR;
	u32 DR;
	u32 BRR;
	u32 CR1;
	u32 CR2;
	u32 CR3;
	u32 GTPR;
}USART_type;


#define USART1       ((volatile USART_type *)(0x0x40013800))

#define BAUD_RATE_9600        0x341
#define BAUD_RATE_115200      0x45

#define PARITY_NO
#define PARITY_YES

#define PARITY_EVEN
#define PARITY_ODD

#define DATA_LEN_8
#define DATA_LEN_9

#define
#endif /* 01_MCAL_USART_USART_PRIVATE_H_ */
