#ifndef GPIO_PRIVATE_H_
#define GPIO_PRIVATE_H_

typedef struct {
	u32 CRL;
	u32 CRH;
	u32 IDR;
	u32 ODR;
	u32 BSSR;
	u32 BRR;
	u32 LCKR;
}GPIO_type;

#define GPIOA        ((volatile GPTO_type *)(0x40010800))
#define GPIOB        ((volatile GPTO_type *)(0x40011000))
#define GPIOC        ((volatile GPTO_type *)(0x40010800))



#define Analog_mode         0b00
#define Flating_Input       0b01
#define Input_PULL_UP_DOWN  0b10



















#endif /* 01MCAL_01GPIO_GPIO_PRIVATE_H_ */
